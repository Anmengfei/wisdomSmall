@font-face {
  font-family: 'iconfont';
  src: url('data:application/font-woff2;charset=utf-8;base64,') format('woff2'), url('data:application/font-woff;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-jiahaoyou:before {
  content: "\e603";
}
.icon-gerenxinxi:before {
  content: "\e511";
}
.icon-faqi:before {
  content: "\e612";
}
.icon-chuli:before {
  content: "\e61d";
}
.complete-wrapper {
  flex: 1;
  z-index: 1;
  display: flex;
  flex-direction: column;
  height: 100%;
}
.complete-content {
  margin-top: 7%;
  /*background: #fbf9fe;*/
  overflow: hidden;
  /*padding-top: 7%;*/
  height: calc(100% - 112rpx);
}
.complete-card {
  overflow: hidden;
  background: #fff;
  margin: 10rpx 20rpx;
  padding: 20rpx 60rpx;
  box-shadow: 0 12rpx 12rpx rgba(75, 92, 178, 0.1);
  border-radius: 4px;
  margin-left: 20rpx;
}
.yangshi {
  display: flex;
  height: 60px;
  line-height: 60rpx;
  justify-content: space-between;
  border-bottom: 3rpx solid rgba(112, 171, 219, 0.6);
}
.yangshititle {
  color: #70abdb;
}
.yangshiTishi {
  color: #6e6d6d;
  font-size: small;
  font-weight: lighter;
}
.yangshivalue {
  color: black;
  font-size: 23rpx;
}
.selectInfo {
  font-size: 30rpx;
  font-weight: bold;
}
.selectFirstInfo {
  text-indent: 1.5em;
  margin: 14rpx 0;
}
.selectFirstInfo span {
  font-size: 22rpx;
}
.complete-return {
  background: #647cfb;
  background: linear-gradient(to right, #4a8ef9 0%, #5292f7 15%, #347ff4 70%, #0c5ddc 100%);
  height: 112rpx;
}
.info-btn {
  background: #347ff4;
  color: #fff;
  width: 140px;
  margin: 20px auto 60px;
  text-align: center;
  border-radius: 4px;
  padding: 6px 10px;
}
.info-confirm-link {
  text-decoration: underline;
}
.demo1-item {
  border: 1px solid #ececec;
  padding: 5px 15px;
  margin: 0 15px 8px;
  font-size: 13px;
}
.demo1-item-selected {
  border: 1px solid green;
}
.info-comfirm {
  margin: 160rpx 0 40rpx;
  font-size: 26rpx;
  opacity: 0.8;
}
.tiaokuan {
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
}
.tiaokuan .mianzetiaokuan {
  text-decoration: underline;
  color: #3c55a8;
}
.tianbao {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 50rpx;
}
.the-label {
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.the-label .th {
  width: 100%;
  overflow: hidden;
}
.the-label .th .ti {
  font-size: 30rpx;
}
.the-label .th .tt {
  color: #b2b2b2;
  font-size: 24rpx;
}
.the-label .list {
  width: 100%;
  overflow: hidden;
  margin-top: 48rpx;
}
.the-label .list .li {
  padding: 0 25rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1px solid #f0f0f0;
  border-radius: 10rpx;
  overflow: hidden;
  font-size: 26rpx;
  color: #808080;
  float: left;
  margin: 0 17rpx 17rpx 0;
  position: relative;
}
.the-label .list .li.cur {
  color: #55beb7;
  border-color: #55beb7;
}
textarea {
  width: 100%;
  height: 50px;
  border: 1px solid #afe2da;
  border-radius: 3px;
}
.img {
  display: inline-block;
}
.pic {
  float: left;
  position: relative;
  margin-right: 9px;
  margin-bottom: 9px;
  width: 100%;
}
.delete-btn {
  position: absolute;
  top: 0;
  right: 0;
}
.weui-uploader {
  padding: 10rpx;
}
.lineHeight {
  width: 100%;
  line-height: 80rpx;
  border-bottom: 1px solid #ccc;
  font-size: 30rpx;
}
.container {
  padding: 0;
  align-items: left;
  padding-left: 15rpx;
}
.numberInfo {
  font-size: 24rpx;
  text-indent: 15rpx;
  border-bottom: 1px solid #ccc;
}
