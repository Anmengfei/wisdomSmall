@font-face {
  font-family: 'iconfont';
  src: url('data:application/font-woff2;charset=utf-8;base64,') format('woff2'), url('data:application/font-woff;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-jiahaoyou:before {
  content: "\e603";
}
.icon-gerenxinxi:before {
  content: "\e511";
}
.icon-faqi:before {
  content: "\e612";
}
.icon-chuli:before {
  content: "\e61d";
}
.xiangqing {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  margin-top: 25rpx;
  padding-top: 10rpx;
  margin-left: 15px;
  padding: 0;
}
.xiangqing_img {
  width: 40%;
  padding: 25rpx;
}
.xiangqing_img image {
  width: 100%;
  height: 220rpx;
}
.xiangqing_info {
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}
.xiangqing_info .zi {
  border: 2rpx #FA8D33 solid;
  color: #FA8D33;
  padding: 2rpx 10rpx;
  font-size: 26rpx;
  margin-right: 10rpx;
  border-radius: 9rpx;
}
.xiangqing_info .title {
  font-weight: 600;
  line-height: 50rpx;
  font-size: 28rpx;
  margin-right: 30rpx;
  color: #404040;
}
.img {
  display: inline-block;
}
.pic {
  float: left;
  position: relative;
  margin-right: 9px;
  margin-bottom: 9px;
}
.pic > image {
  width: 120px;
  height: 120px;
}
.pic-deal {
  float: left;
  position: relative;
  margin-right: 9px;
  margin-bottom: 9px;
  width: 100%;
}
.delete-btn {
  position: absolute;
  top: 0;
  right: 0;
}
.weui-uploader {
  padding: 10rpx;
}
.weui-uploader-deal {
  padding: 10rpx;
}
.threeTabs {
  display: flex;
  justify-content: space-around;
  margin-top: 10px;
}
.tab {
  border: 1px solid #b4b2b2;
  box-shadow: 2px 2px 2px #b4b2b2;
  border-radius: 8rpx;
  width: 100rpx;
  height: 50rpx;
  text-align: center;
}
.chulizhong {
  background-color: #87b4d8;
}
.yiwancheng {
  background-color: #39ec75;
}
.yangshi {
  display: flex;
  height: 60px;
  line-height: 60rpx;
  justify-content: space-between;
  border-bottom: 3rpx solid rgba(112, 171, 219, 0.6);
}
.yangshititle {
  color: #70abdb;
}
.yangshiTishi {
  color: #6e6d6d;
  font-size: small;
  font-weight: lighter;
}
.yangshivalue {
  color: black;
  font-size: 23rpx;
}
.dealForm {
  margin-top: 10px;
}
textarea {
  width: 100%;
  height: 50px;
  border: 1px solid #afe2da;
  border-radius: 3px;
}
.tianbao {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 50rpx;
}
